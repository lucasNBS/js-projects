generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ShortUrlSchema {
  id     Int    @id @default(autoincrement())
  full   String
  short  String @default(uuid())
  clicks Int    @default(0)
}
